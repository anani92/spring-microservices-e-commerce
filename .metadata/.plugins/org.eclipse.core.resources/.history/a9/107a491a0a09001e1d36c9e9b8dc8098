-- Create the wallet_item table
CREATE TABLE wallet_item (
  id INT PRIMARY KEY AUTO_INCREMENT,
  shop_id BIGINT,
  credits DECIMAL(10, 2)
);

-- Insert sample data into the wallet_item table
INSERT INTO wallet_item (shop_id, credits) VALUES (1, 1000.00);
INSERT INTO wallet_item (shop_id, credits) VALUES (2, 500.50);
INSERT INTO wallet_item (shop_id, credits) VALUES (3, 750.25);
INSERT INTO wallet_item (shop_id, credits) VALUES (4, 1200.75);
INSERT INTO wallet_item (shop_id, credits) VALUES (5, 900.50);
